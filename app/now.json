{
  "version": 2,
  "name": "pomodoro.cc",
  "alias": "pomodoro.cc",
  "builds": [{
    "src": "package.json", "use": "@now/static-build"
  }],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/login", "dest": "/index.html" },
    { "src": "/logout", "dest": "/index.html" },
    { "src": "/export", "dest": "/index.html" },
    { "src": "/support", "dest": "/index.html" },
    { "src": "/statistics", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" },
    { "src": "/.*", "status": 404, "dest": "/index.html" }
  ]
}