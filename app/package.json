{
  "name": "pomodoro.cc-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy": "npm run build; aws s3 cp --recursive --acl public-read --region eu-central-1 ./build/ s3://pomodoro.cc",
    "start": "NODE_ENV=development parcel src/index_template.html",
    "build": "rm build/*; NODE_ENV=production webpack --config webpack-prod.config.js --progress --colors",
    "build-watch": "NODE_ENV=test webpack --progress --colors --watch",
    "test": "jest",
    "lint": "standard",
    "test-watch": "karma start --single-run=false"
  },
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "notifyjs": "^3.0.0",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "recharts": "^1.5.0",
    "redux": "^4.0.1",
    "redux-localstorage": "0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "description": "Install the dependencies first with",
  "main": "src/index.js",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-preact": "^1.1.0",
    "css-loader": "^2.1.1",
    "expect": "^23.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.4.5",
    "parcel-bundler": "^1.12.3",
    "r2": "git+https://github.com/mikeal/r2.git",
    "react-loader": "^2.4.5",
    "sinon": "^7.3.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "optionalDependencies": {
    "standard": "^10.0.3"
  },
  "keywords": [],
  "author": "Christian Fei",
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christian-fei/pomodoro.cc.git"
  },
  "bugs": {
    "url": "https://github.com/christian-fei/pomodoro.cc/issues"
  },
  "homepage": "https://github.com/christian-fei/pomodoro.cc#readme",
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  }
}
