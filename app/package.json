{
  "name": "pomodoro.cc-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy": "npm run build; aws s3 cp --recursive --acl public-read --region eu-central-1 ./build/ s3://pomodoro.cc",
    "stack:apply": "up stack plan -v && up stack apply -v",
    "start": "NODE_ENV=development webpack-dev-server --port 9000 --config ./webpack.config.js --hot --progress --colors --history-api-fallback --content-base build-dev",
    "build": "rm build/*; NODE_ENV=production webpack --config webpack-prod.config.js --progress --colors",
    "build-watch": "NODE_ENV=test webpack --progress --colors --watch",
    "test": "karma start",
    "lint": "standard",
    "test-watch": "karma start --single-run=false"
  },
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "notifyjs": "^3.0.0",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "react-dnd": "^6.0.0",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.0",
    "recharts": "^1.4.1",
    "redux": "^4.0.1",
    "redux-localstorage": "0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "description": "Install the dependencies first with",
  "main": "src/index.js",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "expect": "^23.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "karma": "^3.1.3",
    "karma-chai": "0.1.0",
    "karma-cli": "1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.4.5",
    "mocha": "^5.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "r2": "git+https://github.com/mikeal/r2.git",
    "react-loader": "^2.4.5",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.10"
  },
  "optionalDependencies": {
    "standard": "^10.0.3"
  },
